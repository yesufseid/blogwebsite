generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model post {
  id      Int    @id @default(autoincrement())
  title String     @db.LongText
  content String   @db.LongText
  img  String? @db.LongText
}

model user {
  id        Int       @id @default(autoincrement())
  email     String    @unique(map: "User_email_key")
  password  String

}


